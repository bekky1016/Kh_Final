<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kh.java.project.projects.board.mapper.BoardMapper">
  <select id="getIssues" resultType="kh.java.project.projects.dto.param.Issue" >
	select * from issue where project_No = #{projectNo} 
	
	and maker = #{id} and pic = #{id}
	
	and name LIKE #{keyword}
  </select>
  <select id="getProgresses" resultType="kh.java.project.projects.dto.param.Progress" >
    select * from progress where no = #{projectNo}
  </select>
  
</mapper>